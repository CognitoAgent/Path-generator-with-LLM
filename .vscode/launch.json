{
    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "OPENAI_API_KEY": "YOUR_API_KEY_HERE",
                "DRIVE_FOLDER_ID": "YOUR_FOLDER_ID_HERE"
            }
        }
    ]
}